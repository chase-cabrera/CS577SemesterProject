# Database Configuration
# Copy this file to .env and fill in your actual values

# MySQL/MariaDB Database Host (RDS endpoint or localhost)
DB_HOST=database-host.rds.amazonaws.com

# Database Name
DB_NAME=fec_contributions

# Database User sent via iMessage
DB_USER=database_user

# Database Password sent via iMessage
DB_PASSWORD=database_password

# Database Port
DB_PORT=3306

AWS_REGION=us-east-1
